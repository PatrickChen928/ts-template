{
  "name": "pkg-name",
  "version": "0.0.0",
  "packageManager": "pnpm@7.1.7",
  "description": "ts template",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "types/index.d.ts",
  "keywords": [
    "TypeScript",
    "template",
    "cjs",
    "mjs",
    "umd"
  ],
  "scripts": {
    "build": "rollup -c",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "coverage": "vitest run --coverage",
    "release": "bumpp --all && pnpm publish",
    "typecheck": "tsc --noEmit",
    "fix": "eslint --fix --ext .js,.ts src tests scripts",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "prepublishOnly": "nr build"
  },
  "files": [
    "dist/",
    "types/"
  ],
  "author": "cyly <786156072@qq.com>",
  "homepage": "https://github.com/ChpShy/pkg-name#readme",
  "bugs": {
    "url": "https://github.com/ChpShy/pkg-name/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ChpShy/pkg-name.git"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./types/index.d.ts"
    }
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged",
    "commit-msg": "pnpm exec esno scripts/verifyCommit.ts $1"
  },
  "lint-staged": {
    "*.{js,ts,tsx,vue,md}": [
      "eslint --fix"
    ]
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.26.0",
    "@antfu/ni": "^0.16.3",
    "@babel/core": "^7.17.8",
    "@babel/preset-env": "^7.16.11",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^21.0.2",
    "@rollup/plugin-json": "^4.1.0",
    "@types/node": "^17.0.30",
    "@vitest/ui": "^0.7.4",
    "bumpp": "^8.2.1",
    "c8": "^7.11.0",
    "conventional-changelog-cli": "^2.2.2",
    "esbuild": "^0.14.27",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "esno": "^0.16.3",
    "lint-staged": "^13.0.1",
    "picocolors": "^1.0.0",
    "prettier": "^2.6.2",
    "rollup": "^2.70.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-dts": "^4.2.0",
    "rollup-plugin-esbuild": "^4.8.2",
    "simple-git-hooks": "^2.7.0",
    "typescript": "^4.1",
    "vite": "^3.0.0",
    "vitest": "^0.21.1"
  }
}
